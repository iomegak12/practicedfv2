{
	"name": "ProcessedOrdersDLS_DS",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ProcessedOrdersDLSConnection",
			"type": "LinkedServiceReference"
		},
		"type": "AzureDataLakeStoreFile",
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": false
			},
			"fileName": {
				"value": "@concat(guid(), '-processed', '.txt')",
				"type": "Expression"
			},
			"folderPath": "processed-files"
		}
	}
}