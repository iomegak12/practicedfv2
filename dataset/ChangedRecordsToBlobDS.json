{
	"name": "ChangedRecordsToBlobDS",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ChangedRecordsBlobLS",
			"type": "LinkedServiceReference"
		},
		"type": "AzureBlob",
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": false
			},
			"fileName": {
				"value": "@concat('changes-', guid(), '.txt')",
				"type": "Expression"
			},
			"folderPath": "processed-results"
		}
	}
}