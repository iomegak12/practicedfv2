{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "practicedfv2"
		},
		"OnPremisesSQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnPremisesSQLServer'"
		},
		"OnlineOrdersHostedInAzureSQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnlineOrdersHostedInAzureSQL'"
		},
		"ProcessedOrdersBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ProcessedOrdersBlobStorage'"
		},
		"ProcessedOrdersBlobStorage_DS_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@concat(guid(), '-', pipeline().RunId, '.txt')"
		},
		"ProcessedOrdersBlobStorage_DS_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "db-data"
		},
		"iomegakvaccess_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://iomegakvv2.vault.azure.net/"
		},
		"PracticeLiftNShiftISRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
			"type": "string",
			"defaultValue": "iomegasqlserverv2.database.windows.net"
		},
		"PracticeLiftNShiftISRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
			"type": "string",
			"defaultValue": "iomegaadmin"
		},
		"PracticeLiftNShiftISRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
			"type": "secureString"
		},
		"ProcessedOrdersDLSConnection_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://iomegadlsv1.azuredatalakestore.net/webhdfs/v1"
		},
		"ProcessedOrdersDLSConnection_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "4499f09c-741e-4057-ad20-e40d33ccd1d0"
		},
		"ProcessedOrdersDLSConnection_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "381a10df-8e85-43db-86e1-8893b075b027"
		},
		"ProcessedOrdersDLSConnection_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "7293f3cb-5705-4319-a0cc-15472af46eed"
		},
		"ProcessedOrdersDLSConnection_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "onpremisesdataintegration-rg"
		},
		"ProcessedOrdersDLS_DS_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "@concat(guid(), '-processed', '.txt')"
		},
		"ProcessedOrdersDLS_DS_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "processed-files"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ProcessedOrdersBlobStorage_connectionString": {
						"value": "[parameters('ProcessedOrdersBlobStorage_connectionString')]"
					},
					"OnPremisesSQLServer_connectionString": {
						"value": "[parameters('OnPremisesSQLServer_connectionString')]"
					},
					"OnlineOrdersHostedInAzureSQL_connectionString": {
						"value": "[parameters('OnlineOrdersHostedInAzureSQL_connectionString')]"
					},
					"iomegakvaccess_properties_typeProperties_baseUrl": {
						"value": "[parameters('iomegakvaccess_properties_typeProperties_baseUrl')]"
					},
					"PracticeLiftNShiftISRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
						"value": "[parameters('PracticeLiftNShiftISRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint')]"
					},
					"PracticeLiftNShiftISRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
						"value": "[parameters('PracticeLiftNShiftISRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName')]"
					},
					"PracticeLiftNShiftISRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
						"value": "[parameters('PracticeLiftNShiftISRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword')]"
					},
					"ProcessedOrdersBlobStorage_DS_properties_typeProperties_fileName": {
						"value": "[parameters('ProcessedOrdersBlobStorage_DS_properties_typeProperties_fileName')]"
					},
					"ProcessedOrdersBlobStorage_DS_properties_typeProperties_folderPath": {
						"value": "[parameters('ProcessedOrdersBlobStorage_DS_properties_typeProperties_folderPath')]"
					},
					"ProcessedOrdersDLSConnection_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('ProcessedOrdersDLSConnection_properties_typeProperties_dataLakeStoreUri')]"
					},
					"ProcessedOrdersDLSConnection_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ProcessedOrdersDLSConnection_properties_typeProperties_servicePrincipalId')]"
					},
					"ProcessedOrdersDLSConnection_properties_typeProperties_tenant": {
						"value": "[parameters('ProcessedOrdersDLSConnection_properties_typeProperties_tenant')]"
					},
					"ProcessedOrdersDLSConnection_properties_typeProperties_subscriptionId": {
						"value": "[parameters('ProcessedOrdersDLSConnection_properties_typeProperties_subscriptionId')]"
					},
					"ProcessedOrdersDLSConnection_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('ProcessedOrdersDLSConnection_properties_typeProperties_resourceGroupName')]"
					},
					"ProcessedOrdersDLS_DS_properties_typeProperties_fileName": {
						"value": "[parameters('ProcessedOrdersDLS_DS_properties_typeProperties_fileName')]"
					},
					"ProcessedOrdersDLS_DS_properties_typeProperties_folderPath": {
						"value": "[parameters('ProcessedOrdersDLS_DS_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}